        </Field>
        <Field Name="nSuccess">
          <DataField>nSuccess</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="nOthers">
          <DataField>nOthers</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>SSISDB</DataSourceName>
        <CommandType>Text</CommandType>
        <CommandText>WITH filtered AS
(
	SELECT [status] FROM [catalog].[executions]
	WHERE 
	([start_time] IS NULL OR CONVERT (datetime, [start_time]) &gt;= @WindowBegin) AND
	([start_time] IS NULL OR CONVERT (datetime, [start_time]) &lt;= DATEADD(day,1,@WindowEnd))-- AND
)
SELECT 
(SELECT COUNT(1) FROM filtered) AS nAll,
(SELECT COUNT(1) FROM filtered WHERE [status] = 2) AS nRunning,
(SELECT COUNT(1) FROM filtered WHERE [status] = 4) AS nFailed,
(SELECT COUNT(1) FROM filtered WHERE [status] = 7) AS nSuccess,
(SELECT COUNT(1) FROM filtered WHERE [status] != 2 AND [status] != 4 AND [status] != 7) AS nOthers</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <QueryParameters>
          <QueryParameter Name="@WindowBegin">
            <Value>=Parameters!WindowBegin.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@WindowEnd">
            <Value>=Parameters!WindowEnd.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <Timeout>0</Timeout>
      </Query>
    </DataSet>
  </DataSets>
  <ReportParameters>
    <ReportParameter Name="WindowBegin">
      <DataType>String</DataType>
      <Prompt>Window Begin</Prompt>
    </ReportParameter>
    <ReportParameter Name="WindowEnd">
